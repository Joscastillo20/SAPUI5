<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
            <Table items="{oDataNorthwind>Orders}" width="700px">
            
                <columns>
                  <Column>  
                    <header>
                        <Label text="{i18n>orderID}"/>
                    </header>
                  </Column>

                  <Column>  
                    <header>
                        <Label text="{i18n>freight}"/>
                    </header>
                  </Column>

                  <Column>  
                    <header>
                        <Label text="{i18n>shipAddress}"/>
                    </header>
                  </Column>                                    
                </columns>

                <items>
                    <ColumnListItem >
